{
  "name": "@permaweb/ao-loader",
  "version": "0.0.22",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:src",
    "build:src": "node esbuild.js",
    "build:types": "tsc src/index.cjs --skipLibCheck --declaration --allowJs --emitDeclarationOnly --outDir dist",
    "test": "npm run build && node --test",
    "test:integration": "npm run build && MODULE_PATH='../dist/index.cjs' node --test"
  },
  "dependencies": {
    "arweave": "^1.14.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "esbuild": "^0.19.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
